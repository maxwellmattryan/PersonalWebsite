{
    "$schema": "./node_modules/@angular/service-worker/config/schema.json",
    "index": "./src/index.html",
    "assetGroups": [
        {
            "name": "ui",
            "installMode": "prefetch",
            "resources": {
                "files": [
                    "./src/favicon.ico",
                    "./src/index.html",
                    "/*.css",
                    "/*.js"
                ]
            }
        },
        {
            "name": "assets",
            "installMode": "lazy",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "./src/assets/*"
                ]
            }
        }
    ]
}